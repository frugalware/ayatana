pkgver=0.74_404001
pkgrel=1
pkgdesc="AMD xvba SDK."
url="http://developer.amd.com/zones/opensource/pages/default.aspx"
depends=()
groups=('devel-extra')
archs=('i686' 'x86_64')
up2date="Flasttar $url"
