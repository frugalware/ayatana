pkgrel=1
pkgdesc="A free CAS (computer algebra system)"
url="http://www.ginac.de/"
depends=('cln>=1.2.2' 'readline')
groups=('apps-extra')
archs=('i686' 'x86_64')
up2date="lynx -dump http://www.ginac.de/Download.html|grep 'ginac-\(.*\).tar.bz2'|grep -v version|sed 's/.*-\(.*\).t.*/\1/'"
source=($url/$pkgname-$pkgver.tar.bz2)
sha1sums=('0c6805f32ffe6a20e0a2aead187a975b1f50480d')

