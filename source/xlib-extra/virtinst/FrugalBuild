pkgname=virtinst
pkgver=0.600.0
pkgrel=1
pkgdesc="virtinst is a module to help in starting installations on Xen"
url="http://virt-manager.et.redhat.com"
depends=('libvirt-python>=0.8.7' 'urlgrabber')
groups=('xlib-extra')
archs=('i686' 'x86_64')
up2date="lynx -dump http://virt-manager.et.redhat.com/download.html | grep -m1 'virtinst [0-9]' | sed 's/.*virtinst //' | sed 's/ .*//'"
source=(http://virt-manager.et.redhat.com/download/sources/virtinst/$pkgname-$pkgver.tar.gz)
