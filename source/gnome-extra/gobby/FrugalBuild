pkgname=gobby
pkgver=0.4.94
pkgrel=3
pkgdesc="Gobby is a collaborative text editor"
url="http://releases.0x539.de/gobby/"
depends=('obby>=0.4.5' 'libgnomeprint>=2.18.0' 'gtkmm2>=2.20.2-2' \
	'gtksourceview>=2.10.0-4' 'libxml++' 'gnome-vfs>=2.18.0' 'libinfinity>=0.5.1')
makedepends=('intltool')
groups=('gnome-extra')
options=('scriptlet')
archs=('i686' 'x86_64')
up2date="lynx -dump $url | sed '/latest/d' | sed '/stable/d' | Flasttar"
source=($url/$pkgname-$pkgver.tar.gz)
sha1sums=('921979da611601ee6e220e2396bd2c86f0fb8c66')
_F_gnome_desktop=y
_F_gnome_iconcache=y
Finclude gnome-scriptlet
Fconfopts="$Fconfopts --with-gnome --with-gtksourceview2"

# optimization OK
