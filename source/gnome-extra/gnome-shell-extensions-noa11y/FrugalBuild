# Compiling Time: 0.01 SBU
# Maintainer: Baste <baste@frugalware.org>

pkgname=gnome-shell-extensions-noa11y
pkgver=20110526
pkgrel=1
pkgdesc="gnome-shell extension to remove the Accessibility icon from the panel."
url="https://github.com/ecoleman/noa11y-colemando.com"
depends=('gnome-shell')
groups=('gnome-extra')
archs=('i686' 'x86_64')
up2date="Flasttar http://ftp.frugalware.org/pub/other/people/devil505/snapshots/$pkgname"
source=(http://ftp.frugalware.org/pub/other/people/devil505/snapshots/$pkgname/$pkgname-$pkgver.tar.bz2)
sha1sums=('6e6dd989bbcd51f942b46ebfd8b7332bd0d0bf47')
Finclude gnome-shell-extensions

build(){
        Fcd
        Fmkdir usr/share/gnome-shell/extensions/noa11y\@colemando.com
        Fcprel extension.js usr/share/gnome-shell/extensions/noa11y\@colemando.com/
        Fcprel metadata.json usr/share/gnome-shell/extensions/noa11y\@colemando.com/
	FGnomeShellVersion
}

